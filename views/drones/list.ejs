<h2>Available drones</h2>
<div> <% droneFromDb.forEach(({name, propellers, maxSpeed, _id}) => { %>
    <h2><%- name %></h2>
    <span><%- propellers %></span>
    <span><%- maxSpeed%></span>
    <a href="/drones/<%- _id  %>/edit">Edit</a>
    <form
class="delete"
method="POST" 
action="/drones/<%- _id %>/delete">
<button id="drone-delete-button" type="submit">Delete</button>
</form>
    
    <button id="drone-edit-button" type="submit">Edit</button>
    <% }) %>
   
</div> 