<h2>Available drones</h2>
<div>{{#each droneFromDb}}
    <h2>{{this.name}}</h2>
    <span>{{this.propellers}}</span>
    <span>{{this.maxSpeed}}</span>
    <a href="/drones/{{this._id}}/edit"></a>
    <button id="drone-delete-button" type="submit">Delete</button>
    <button id="drone-edit-button" type="submit">Edit</button>
    {{/each}}
   
</div>